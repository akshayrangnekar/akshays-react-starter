{
  "name": "akshays-react-starter",
  "version": "1.0.0",
  "description": "A simple React starter that includes React, Redux, Mocha, Webpack, Babel and a few other useful starting points",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "webpack --config ./internal/webpack/webpack.config.js",
    "dev": "webpack-dev-server --env development --config ./internal/webpack/webpack.config.js"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate"
  ],
  "author": "Akshay Rangnekar",
  "license": "ISC",
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "redux-devtools": "^3.4.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": "true",
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-console": 0,
      "react/prop-types": 0
    }
  }
}
